{
  "name": "@fieldmade/prototype",
  "version": "0.0.1",
  "private": true,
  "description": "FieldNode Web Prototype - built with remix run",
  "license": "ISC",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "clean": "npx rimraf build && npx rimraf public/build",
    "dead": "ts-prune | (! grep -v 'used in module')"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not dead",
      "not ie > 0",
      "not ie_mob > 0",
      "not op_mini all"
    ],
    "development": [
      ">0.4%",
      "not dead",
      "not ie > 0",
      "not ie_mob > 0",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@carbon/icons-react": "^11.3.0",
    "@fieldmade/ui": "^1.2.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "tailwind-merge": "^1.5.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "material-ui-colors": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || ^16.10.0 || >=17.0.0"
  },
  "main": ".eslintrc.js",
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/fieldmade/di-webapp/issues"
  },
  "homepage": "https://github.com/fieldmade/di-webapp#readme"
}
